{
  "name": "jest-selenium-demo",
  "version": "1.0.0",
  "description": "This project shows how to drive your selenium tests with Jest.",
  "main": "index.js",
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./setupFile.js"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./html-report",
          "filename": "tests-report.html",
          "pageTitle": "360 Test Report",
          "logoImgPath": "./360logo.png",
          "hideIcon": true,
          "expand": true
        }
      ]
    ]
  },
  "scripts": {
    "test": "jest --maxWorkers=50%"
  },
  "author": "Haider Ali",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.8.7",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.1.0",
    "jest-cli": "^25.2.0",
    "jest-html-reporters": "^2.0.4",
    "selenium-webdriver": "^3.6.0"
  },
  "dependencies": {
    "chromedriver": "^85.0.0",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "geckodriver": "^1.19.1",
    "log4js": "^6.3.0",
    "node-cache": "^5.1.0",
    "properties-reader": "^1.0.0"
  }
}
